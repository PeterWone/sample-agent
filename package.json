{
	"name": "sample-agent",
	"displayName": "Sample agent",
	"description": "Allows remote extension to execute an observable action on the workstation.",
	"version": "0.0.1",
	"author": {
		"name": "Peter Wone",
		"email": "peter.wone@outlook.com"
	},
	"publisher": "pdconsec",
	"repository": {
		"type": "git",
		"url": "https://github.com/PeterWone/sample-agent"
	},
	"engines": {
		"vscode": "^1.74.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:sample-agent.showInvokerHost"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"commands": [
			{
				"category": "Agent",
				"title": "Show invoker host",
				"command": "sample-agent.showInvokerHost"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"compile": "npm run webpack",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.46.0",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"css-loader": "^6.3.0",
		"eslint": "^7.9.0",
		"file-loader": "^6.2.0",
		"glob": "^7.1.6",
		"html-loader": "^3.1.2",
		"mocha": "^8.1.3",
		"ts-loader": "^9.4.1",
		"typescript": "^4.0.2",
		"vscode-test": "^1.4.0",
		"webpack": "^5.75.0",
		"webpack-cli": "^4.10.0"
	},
	"dependencies": {
	}
}